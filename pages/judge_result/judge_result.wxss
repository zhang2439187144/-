.butt {
  width: 80%;
  display: flex;
  flex-direction: row;
  margin-left: 7%;
  margin-bottom: 20rpx;
}

.butto {
  width: 80%;
  height: 85rpx;
  border-radius: 30px;
  background: #673bb7;
  color: white;
  margin-left: 10%;
  line-height: 85rpx;
  font-size: 40rpx;
  font-family: '黑体';
}

.butto::after {
  border: none;
}

.hoverclass {
  background-color: #663bb757;
}

.tab {
  display: flex;
  flex-direction: column;
  
}

.tab-nav {
  height: 80rpx;
  background: #fff;
  border-bottom: 0.5rpx dotted #ddd;
  display: flex;
  line-height: 79rpx;
  position: relative;
  margin-bottom: 30rpx;
  text-align: center;
}

.tab-line {
  position: absolute;
  left: 0;
  bottom: -1rpx;
  height: 6rpx;
  background: #1296d2;
  transition: all 0.3s;
}

.tab-content {
  flex: 1;
  overflow-y: auto;
  overflow-x: hidden;
}

.detail-cont {
  width: 98%;
}

.items {
  /* display: flex; */
