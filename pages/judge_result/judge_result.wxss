}

.keshi {
  font-size: 40rpx;
  line-height: 90rpx;
  padding: 0 10rpx;
  border-radius: 10rpx;
  /* font-weight:bold; */
  background-color: rgba(128, 128, 128, 0.116);
}

.contentt {
  font-size: 33rpx;
  color: gray;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2; /*显示的行数 */
}

.content-temp {
  font-size: 33rpx;
  color: gray;
}

.state {
